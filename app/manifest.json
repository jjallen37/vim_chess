{
  "name": "__MSG_appName__",
  "version": "5.4.0",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "default_locale": "en",
  "permissions": [
    "https://www.chess.com/"
  ],
  "browser_action": {
    "default_icon": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.chess.com/*"
      ],
      "js": [
        "inject.js"
      ],
      "run_at": "document_end",
      "css": [
        "styles/main.css"
      ]
    }
  ],
  "web_accessible_resources": [
    "build.js"
  ]
}
